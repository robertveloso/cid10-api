{
    "name": "atiladadanvi/cid10-api",
    "description": "Api com todas as doen√ßas registradas pela OMS.",
    "authors": [
        {
            "name": "Atila Silva",
            "email": "atila.danvi@outlook.com"
        }
    ],
    "version":"1.1.0",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2.5  ",
        "ext-json": "*",
        "barryvdh/laravel-cors": "^0.11.2",
        "laravel/lumen-framework": "7.0.*",
        "spatie/laravel-fractal": "^5.4",
        "vlucas/phpdotenv": "~3.3"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "classmap": [
            "database/seeds"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
